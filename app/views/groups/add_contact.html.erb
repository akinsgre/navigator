<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="span6 " >
    <%= form_for(@contact, :url => groups_save_contact_path) do |f|  %>

    <%= f.label :entry, "Email Address" %>
      <%= f.text_field :entry %>

      <%= f.label :identifier, "Contact Description" %>
      <%= f.text_field :identifier %>
      <br>

      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
