
<div id="addTypes" >
<div class="resetLayout" >
  <%= form_for @contact, :url => group_contacts_path(@group), :validate => true do |f| %>
  <%= f.error_messages %>
    <%= f.hidden_field :user_id %>
    <% unless @group.nil? %>
       <%= f.fields_for @group do |fd| %>
       <%= fd.hidden_field :id %>
       <% end %>
    <% end %>
  <p><div class="row-fluid"><div class="span11">
    <%= f.label :name %><br /></div></div>
   <div class="row-fluid"><div class="span11"> <%= f.text_field :name %>
  </p>
<p></div></div>
<div class="row-fluid"><div class="span2">
  Preferred method of contact</div>
<div class="span10" float="left">
  <%= f.select "type", options_for_select(Contact.select_options) %>
</p>
 <p>
</div></div>
   <div class="row-fluid"><div class="span11"> <label id="entryLabel" for="entry">Entry Number or email address</label><br /></div></div>
   <div class="row-fluid"><div class="span11">
    <%= f.text_field :entry %></div></div>
  </p>
<div class="row-fluid">
<div class="span11" float="left">
  <p><%= f.submit %></p>
<% end %>
</div></div>
</div>
</div>
